<web-app>

<servlet>
<servlet-name>login</servlet-name>
<servlet-class>MVCexam.Controller.LoginController</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>login</servlet-name>
<url-pattern>/login</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>questions</servlet-name>
<servlet-class>MVCexam.Controller.QuestionController</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>questions</servlet-name>
<url-pattern>/questions</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>answers</servlet-name>
<servlet-class>MVCexam.Controller.AnswerController</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>answers</servlet-name>
<url-pattern>/answers</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>marks</servlet-name>
<servlet-class>MVCexam.Controller.ReportController</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>marks</servlet-name>
<url-pattern>/getreport</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>admin</servlet-name>
	<servlet-class>MVCexam.Controller.AdminServ</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>admin</servlet-name>
	<url-pattern>/admin</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>s6</servlet-name>
	<servlet-class>MVCexam.Controller.AjaxHandler</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>s6</servlet-name>
	<url-pattern>/handler</url-pattern>
</servlet-mapping>

	

<listener>
	<listener-class>MVCexam.Controller.SessionCounter</listener-class>
</listener>
<welcome-file-list>  
      <welcome-file>Login.html</welcome-file>  
</welcome-file-list>

</web-app>